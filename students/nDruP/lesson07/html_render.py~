class Element:
    tag = "Element"

    
    def __init__(self):
        self.content = ()


    def render(memory, offset):
        for x in content:
            memory.write(x+offset)
        

    def append(self, text):
        mutable_content = list(self.content)
        mutable_content.append(text)
        self.content = tuple(mutable_content)
